{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.4.0"
    }
  },
  "routes": [
    { "src": "/",  "dest": "/api" },
    { "src": "/img", "dest": "/api/img" },
    { "src": "/data", "dest": "/api/data" },
    { "src": "/retro", "dest": "/api/retro" },
    { "src": "/media", "dest": "/api/media" },
    { "src": "/misc", "dest": "/api/misc" }
  ]
}